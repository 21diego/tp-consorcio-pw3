
@{
    ViewBag.Title = "_UsuarioLayout";
    Layout = "~/Views/Shared/_BaseLayout.cshtml";
}
@Scripts.Render("~/js/DataTables/js")
@Styles.Render("~/js/DataTables/css")
@Scripts.Render("~/js")
@Scripts.Render("~/bundles/jqueryval")
@RenderSection("scripts", required: false)
<div class="container">
    <nav class="navbar navbar-expand-lg navbar-dark bg-primary">
        @Html.ActionLink("S.A.C", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarColor01" aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarColor01">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item active">
                <li>@Html.ActionLink("Inicio", "Index", "Home", new { area = "" }, new { @class = "nav-link" })</li>
                <li class="nav-item">
                    @Html.ActionLink("Mis Consorcios", "Index", "Consorcio", new { area = "" }, new { @class = "nav-link" })
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#">Salir</a>
                </li>
            </ul>
        </div>
    </nav>
</div>
<div class="container body-content">
    @RenderBody()
</div>